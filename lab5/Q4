/*
Write a nested if statement for the decision diagrammed in the
accompanying flowchart. Use a multiple-alternative if statements for
intermediate decisions where possible.
*/

int main(){
	int ph;
	printf("Enter ph:");
	scanf("%d", &ph);
	
	if (ph>7){
		if (ph<12){
			printf("Alkaline");
		}
		else{
			printf("Very alkaline");
		}
	}
	else if (ph==7){
		printf("Neutral");
	}
	else{
		if (ph>2){
			printf("Acidic");
		}
		else{
			printf("Very acidic");
		}
	}
	return 0;
}
